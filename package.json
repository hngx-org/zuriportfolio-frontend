{
  "name": "zuriportfolio",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && tsc --noEmit",
    "format": "npx prettier --write .",
    "postinstall": "husky install",
    "genMail": "cd scripts && ts-node mjmlToHtml.ts"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "axios": "^1.5.1",
    "class-variance-authority": "^0.7.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "iconsax-react": "^0.0.8",
    "jwt-decode": "^3.1.2",
    "mjml": "^4.14.1",
    "next": "^13.5.4",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.4",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "typescript": "4.9.4"
  },
  "description": "zuriportfolio frontend",
  "devDependencies": {
    "@types/mjml": "^4.7.2",
    "@types/node": "18.11.18",
    "@types/nprogress": "^0.2.1",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "autoprefixer": "10.4.16",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
